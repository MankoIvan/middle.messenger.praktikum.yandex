{
    "extends": "xo",
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "max-len": [2, 100],
        "@typescript-eslint/no-unused-vars": 2
    },
    "env": {
      "browser": true,
      "mocha": true
    },
    "globals": {
      "document": true
    }
    
}